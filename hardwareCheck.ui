QComboBox {
    background-color: #FFF;
    border: 2px solid #9DB4C9;
    border-top-left-radius: 8px;
    border-bottom-right-radius: 8px;
    padding: 4px 24px 4px 8px; /* leave room on the right for the arrow */
    min-height: 24px;
}

/* 2) Hover / focus states on the closed combo */
QComboBox:hover {
  border-color: #2934CD;
  /* remove the invalid 'border: 8px;' */
}

/*
QComboBox:focus {
    border-color: #337ab7;
    box-shadow: 0 0 3px rgba(51,122,183,0.5);
}
 */
/* 3) The drop-down arrow container */
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left: 2px solid #FFFF;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
}

/* 4) The arrow itself */
QComboBox::down-arrow {
    image: url(:/icons/icons/arrow.png);
    width: 12px; height: 12px;
}

/* 6) The popup list view */
/* popup list */
QComboBox QAbstractItemView {
  background-color: #FFFFFF;    /* or simply 'white' */
  border: 2px solid #9DB4C9;
  selection-background-color: #D5EAEB;
  selection-color: white;
  padding: 2px;
  outline: none;
}

/* 7) Individual items in the popup */
QComboBox QAbstractItemView::item {
    padding: 4px 8px;
}
QComboBox QAbstractItemView::item:hover {
    background-color: #28323A;
}
QComboBox QAbstractItemView::item:selected {
    background-color: #86B3DB;
    color: white;
}

/* 8) Custom scrollbar in the popup (optional) */
QComboBox QAbstractItemView QScrollBar:vertical {
    background: #f0f0f0;
    width: 8px;
    margin: 0px 2px 0px 2px;
}
QComboBox QAbstractItemView QScrollBar::handle:vertical {
    background: #bbb;
    min-height: 20px;
    border-radius: 4px;
}
QComboBox QAbstractItemView QScrollBar::add-line,
QComboBox QAbstractItemView QScrollBar::sub-line {
    height: 0px;  /* hide arrow buttons */
}
QComboBox QAbstractItemView QScrollBar::add-page,
QComboBox QAbstractItemView QScrollBar::sub-page {
    background: none;
}

